{
    "name": "asseco-voice/laravel-common",
    "description": "Laravel common",
    "type": "library",
    "keywords": [
        "php",
        "laravel",
        "microservices",
        "database"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "asseco-voice/laravel-blueprint-audit": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Common\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:ibnu-ja/laravel-blueprint-audit.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Common\\CommonServiceProvider"
            ]
        }
    }
}
